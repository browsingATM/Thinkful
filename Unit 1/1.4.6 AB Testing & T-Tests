import pandas as pd
import math
#1
meanv = [5, 8]
stdv = [1, 3]
sample = [200, 500]

t_top_value = (meanv[0] - meanv[1])
grp1 = (math.pow(stdv[0], 2))/sample[0]
grp2 = (math.pow(stdv[1], 2))/sample[1]
t_bottom_value = math.sqrt(grp1+grp2)
final_v = t_top_value / t_bottom_value
print(final_v)

answer: -19.78141420187361
--
